{
    "name": "skeb-ext",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://skeb.jp/*"],
            "css": [],
            "js": ["script.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "storage",
        "activeTab",
        "https://skeb.jp/*"
    ]
}
